{
    "id": "bitsearch",
    "name": "Bitsearch Audio",
    "version": "1.8.0",
    "providerType": "api",
    "description": "Bitsearch torrent search - Audiobooks (cat 8) and Other Audio (cat 1/sub 1) - Feb 2026",
    "search": {
        "url": "https://bitsearch.to/search?q={query}&category=8",
        "additionalUrls": [
            "https://bitsearch.to/search?q={query}&category=1&subCategory=1"
        ],
        "method": "GET",
        "headers": {
            "User-Agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/120.0.0.0 Safari/537.36"
        },
        "responseType": "html",
        "resultSelector": "<div[^>]*class=\"[^\"]*bg-white[^\"]*rounded-lg[^\"]*shadow[^\"]*\"[^>]*>([\\s\\S]*?)</div>\\s*(?=<div[^>]*class=\"[^\"]*bg-white[^\"]*rounded-lg[^\"]*shadow|$)",
        "fields": {
            "title": "<a[^>]*href=\"/torrent/[^\"]*\"[^>]*>([^<]+)</a>",
            "detailUrl": "href=\"(/torrent/[^\"]+)\"",
            "size": "fa-download[^>]*></i>\\s*<span>([^<]+)</span>",
            "seeds": "text-green-600[^>]*>[\\s\\S]*?font-medium[^>]*>(\\d+)",
            "peers": "text-red-600[^>]*>[\\s\\S]*?font-medium[^>]*>(\\d+)",
            "magnetUrl": "href=\"(magnet:\\?xt=urn:btih:[^\"]+)\""
        }
    },
    "notes": [
        "v1.8.0 Feb 2026: Fixed size regex - uses fa-download icon (not fa-hdd)",
        "Size structure: fa-download></i><span>284.22 MB</span>",
        "Seeds: text-green-600 container > font-medium > number",
        "Peers: text-red-600 container > font-medium > number"
    ]
}