{
    "id": "bitsearch",
    "name": "Bitsearch Audio",
    "version": "1.2.0",
    "providerType": "api",
    "description": "Bitsearch torrent search with audio category filter - Updated Jan 2026",
    "search": {
        "url": "https://bitsearch.to/search?q={query}&category=8",
        "method": "GET",
        "headers": {
            "User-Agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/120.0.0.0 Safari/537.36"
        },
        "responseType": "html",
        "resultSelector": "<div[^>]*class=\"[^\"]*bg-white[^\"]*rounded-lg[^\"]*shadow-sm[^\"]*border[^\"]*p-6[^\"]*\"[^>]*>([\\s\\S]*?)</div>\\s*</div>\\s*</div>",
        "fields": {
            "title": "<h3[^>]*>\\s*<a[^>]*>\\s*([^<]+?)\\s*</a>",
            "size": "<i[^>]*class=\"[^\"]*fa-download[^\"]*\"[^>]*></i>\\s*<span>([^<]+)</span>",
            "seeds": "text-green-600[^>]*>\\s*<i[^>]*></i>\\s*<span[^>]*>(\\d+)</span>\\s*<span>seeders</span>",
            "peers": "text-red-600[^>]*>\\s*<i[^>]*></i>\\s*<span[^>]*>(\\d+)</span>\\s*<span>leechers</span>",
            "magnetUrl": "href=\"(magnet:\\?[^\"]+)\""
        }
    },
    "notes": [
        "Updated Jan 2026 based on HTML inspection",
        "Bitsearch category 8 = Audio/Audiobooks",
        "Magnet links are available directly on search results",
        "Title in h3 > a, size after fa-download icon",
        "Seeds in span.text-green-600, leechers in span.text-red-600"
    ]
}